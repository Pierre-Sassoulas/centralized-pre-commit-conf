{
  "code-block-style": false,
  "default": true,
  "first-heading-h1": false,
  "hr-style": false,
  "line-length": false,
  "no-duplicate-heading": false,
  "no-inline-html": {
    "allowed_elements": [
      "a",
      "br",
      "sup",
      "sub",
      "iframe",
      "img",
      "figure",
      "figcaption"
    ]
  },
  "no-trailing-punctuation": false,
  "ol-prefix": false,
  "ul-indent": {
    "indent": 2
  }
}
